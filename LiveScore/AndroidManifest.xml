<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tse.livescore.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tse.livescore.activity.Launch_Page"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tse.livescore.activity.Main_Page"
            android:label="@string/title_activity_main__page" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.ListViewLoader"
            android:label="@string/title_activity_list_view_loader" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.Detail_Live_Page"
            android:label="@string/title_activity_detail__live__page"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.MyTabListener"
            android:label="@string/title_activity_my_tab_listener" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.Tab_Page"
            android:label="@string/title_activity_tab__page" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.New_Live_Page"
            android:label="@string/title_activity_new__live__page" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.List_By_Competition"
            android:label="@string/title_activity_list__by__competition" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.Search_Page"
            android:label="@string/title_activity_search__page" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.List_By_Date"
            android:label="@string/title_activity_list__by__date" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.List_By_Depar_And_Sport"
            android:label="@string/title_activity_list__by__depar__and__sport" >
        </activity>
        <activity
            android:name="com.tse.livescore.activity.Single_sport"
            android:label="@string/title_activity_single_sport" >
        </activity>
    </application>

</manifest>